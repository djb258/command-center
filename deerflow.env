GOOGLE_CLIENT_SECRET=your-google-client-secret-here
GOOGLE_REDIRECT_URI=https://your-app-domain.com/oauth2callback

# =============================================================================
# ü§ñ MINDPAL AI AGENT CONFIGURATION
# =============================================================================
MINDPAL_API_KEY=your-mindpal-api-key-here
MINDPAL_AGENT_ID=your-mindpal-agent-id-here
MINDPAL_BASE_URL=https://api.mindpal.com/v1

# =============================================================================
# üöÄ RENDER DEPLOYMENT CONFIGURATION
# =============================================================================
RENDER_API_KEY=your-render-api-key-here
RENDER_WEBHOOK_URL=https://your-render-webhook-url-here
RENDER_SERVICE_ID=your-render-service-id-here
RENDER_ENVIRONMENT=production

# =============================================================================
# üß† LLM CONFIGURATION
# =============================================================================
LLM_PROVIDER=openai
LLM_API_KEY=your-openai-api-key-here
LLM_MODEL=gpt-3.5-turbo

# Alternative LLM providers (uncomment and configure as needed)
# ANTHROPIC_API_KEY=your-anthropic-api-key-here
# GEMINI_API_KEY=your-google-gemini-api-key-here

# =============================================================================
# üï∑Ô∏è WEB SCRAPING CONFIGURATION
# =============================================================================
BROWSERLESS_API_KEY=your-browserless-api-key-here
BROWSERLESS_BASE_URL=https://chrome.browserless.io

# =============================================================================
# üîß ORCHESTRATION SETTINGS
# =============================================================================
API_TIMEOUT=60000
RETRY_ATTEMPTS=3
DEBUG=false
LOG_LEVEL=info

# =============================================================================
# üîí BARTON DOCTRINE ENFORCEMENT
# =============================================================================
BARTON_DOCTRINE_STRICT_MODE=true
BARTON_DOCTRINE_NUCLEAR_ENFORCEMENT=false
BARTON_DOCTRINE_VALIDATION_ENABLED=true

# =============================================================================
# üìä MONITORING & LOGGING
# =============================================================================
ENABLE_ORCHESTRATION_LOGGING=true
ENABLE_FIREBASE_AUDIT_TRAIL=true
ENABLE_PERFORMANCE_METRICS=true 